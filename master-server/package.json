{
  "name": "supraball-master-server-emulator",
  "version": "2.0.0",
  "description": "Master Server Emulator for Supraball with Lobby System",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node build.js",
    "build:exe": "node build-exe.js"
